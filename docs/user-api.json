{
    "openapi": "3.0.3",
    "info": {
        "title": "User API",
        "description": "User API",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000"
        }
    ],
    "paths": {
        "/api/users": {
            "post": {
                "description": "Register new user",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "examples": {
                                "valinaevelyn": {
                                    "description": "Register user Valina Evelyn",
                                    "value": {
                                        "name": "Valina Evelyn",
                                        "username": "valinaevelyn",
                                        "password": "rahasia",
                                        "gender": 1,
                                        "imageKTP": "3827140132.jpeg",
                                        "dob": "2004-01-1",
                                        "role": 0
                                    }
                                }
                            },
                            "schema": {
                                "type": "object",
                                "required": ["username", "email","password","name","gender","imageKTP","dob","role"],
                                "properties": {
                                    "idUser": {
                                        "type": "number"
                                    },
                                    "username": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string"
                                    },
                                    "gender": {
                                        "type": "integer"
                                    },
                                    "imageKTP": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "dob": {
                                        "type": "string",
                                        "format": "date"
                                    },
                                    "phonenumber": {
                                        "type": "string"
                                    },
                                    "profilePicture": {
                                        "type": "string"
                                    },
                                    "role": {
                                        "type": "number"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "examples": {
                                    "validationerror": {
                                        "description": "Validation error",
                                        "value": {
                                            "errors": {
                                                "username": [
                                                    "username must not be blank",
                                                    "username min 6 characters"
                                                ],
                                                "password": [
                                                    "username must not be blank",
                                                    "password min 6 characters"
                                                ],
                                                "name": [
                                                    "name must not be blank"
                                                ],
                                                "email": [
                                                    "email must not be blank"
                                                ],
                                                "gender": [
                                                    "email must not be blank"
                                                ],
                                                "imageKTP": [
                                                    "imageKTP must not be blank"
                                                ],
                                                "dob": [
                                                    "dob must not be blank"
                                                ],
                                                "role": [
                                                    "role must not be blank"
                                                ]
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "201": {
                        "description": "Success register user",
                        "content": {
                            "application/json": {
                                "examples": {
                                    "success": {
                                        "description": "Success register user",
                                        "value": {
                                            "data": {
                                                "idUser": 1,
                                                "name": "Valina Evelyn",
                                                "username": "valinaevelyn",
                                                "password": "rahasia",
                                                "gender": 1,
                                                "imageKTP": "3827140132.jpeg",
                                                "dob": "2004-01-1",
                                                "role": 0
                                            }
                                        }
                                    }
                                },
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "idUser": {
                                                    "type": "number"
                                                },
                                                "username": {
                                                    "type": "string"
                                                },
                                                "password": {
                                                    "type": "string"
                                                },
                                                "name": {
                                                    "type": "string"
                                                },
                                                "gender": {
                                                    "type": "integer"
                                                },
                                                "imageKTP": {
                                                    "type": "string"
                                                },
                                                "email": {
                                                    "type": "string"
                                                },
                                                "dob": {
                                                    "type": "string",
                                                    "format": "date"
                                                },
                                                "phonenumber": {
                                                    "type": "string"
                                                },
                                                "profilePicture": {
                                                    "type": "string"
                                                }
                                                "role": {
                                                    "type": "number"
                                                }
                                            }
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
        ,
        "/api/users/login": {
            "post": {
                "description": "Login user",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "username": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success login",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "idUser": {
                                                    "type": "number"
                                                },
                                                "username": {
                                                    "type": "string"
                                                },
                                                "name": {
                                                    "type": "string"
                                                },
                                                "token": {
                                                    "type": "string"
                                                }
                                            }
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/users/current": {
            "get": {
                "description": "Get current user",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success get current user",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "idUser": {
                                                    "type": "number"
                                                },
                                                "username": {
                                                    "type": "string"
                                                },
                                                "password": {
                                                    "type": "string"
                                                },
                                                "name": {
                                                    "type": "string"
                                                },
                                                "gender": {
                                                    "type": "integer"
                                                },
                                                "imageKTP": {
                                                    "type": "string"
                                                },
                                                "email": {
                                                    "type": "string"
                                                },
                                                "dob": {
                                                    "type": "string",
                                                    "format": "date"
                                                },
                                                "phonenumber": {
                                                    "type": "string"
                                                },
                                                "profilePicture": {
                                                    "type": "string"
                                                }
                                            }
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "patch": {
                "description": "Update current user",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header"
                    }
                ],
                "requestBody": {
                    "description": "Update current user",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "username": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "dob": {
                                        "type": "string",
                                        "format": "date"
                                    },
                                    "phonenumber": {
                                        "type": "string"
                                    },
                                    "profilePicture": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success update user",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "idUser": {
                                                    "type": "number"
                                                },
                                                "username": {
                                                    "type": "string"
                                                },
                                                "email": {
                                                    "type": "string"
                                                },
                                                "dob": {
                                                    "type": "string",
                                                    "format": "date"
                                                },
                                                "phonenumber": {
                                                    "type": "string"
                                                },
                                                "profilePicture": {
                                                    "type": "string"
                                                },
                                                "name": {
                                                    "type": "string"
                                                },
                                                "password": {
                                                    "type": "string"
                                                }
                                            }
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/users/logout": {
            "delete": {
                "description": "Logout current user",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success logout user",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "boolean"
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
